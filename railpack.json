{
  "$schema": "https://schema.railpack.com",
  "provider": "node",
  "packages": {
    "node": "22",
    "pnpm": "9.0.0"
  },
  "steps": {
    "build": {
      "commands": [ "npx -y @redplanethq/holo build" ]
    }
  },
  "deploy": {
    "inputs": [ { "step": "build" } ],
    "startCommand": "cd .main/apps/holo && node --version &&  npm run start"
  }
}